/dts-v1/;

#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "tplink,tl-wdr8610-v3", "mediatek,mt7621-soc";
	model = "TP-Link TL-WDR8610 v3";

	aliases {
		led-boot = &power;
		led-failsafe = &power;
		led-running = &power;
		led-upgrade = &power;
		label-mac-device = &wan;
	};

	leds {
		compatible = "gpio-leds";

		power: power {
			label = "tl-wdr8610-v3:white:power";
			gpios = <&gpio 25 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};
};

&spi0 {
	status = "okay";

	w25q64@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <40000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x000000 0x01d000>;
				read-only;
			};

			config: partition@1d000 {
				label = "config";
				reg = <0x1d000 0x1000>;
				read-only;
			};

			radio: partition@1e000 {
				label = "radio";
				reg = <0x1e000 0x2000>;
				read-only;
			};

			firmware@20000 {
				label = "firmware";
				compatible = "tplink,firmware";
				reg = <0x020000 0x7c0000>;
			};
		};
	};
};

&pcie {
	status = "okay";
};

&pcie0 {
	mt76@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&radio 0>;
		mtd-mac-address = <&config 0x080d>;
		mtd-mac-address-increment = <2>;
		ieee80211-freq-limit = <2400000 2500000>;
	};
};

&pcie1 {
	mt76@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&radio 0x1000>;
		mtd-mac-address = <&config 0x080d>;
		mtd-mac-address-increment = <3>;
		ieee80211-freq-limit = <5000000 6000000>;
	};
};

&gmac0 {
	mtd-mac-address = <&config 0x080d>;
	mtd-mac-address-increment = <1>;
};

&switch0 {
	ports {
		wan: port@0 {
			status = "okay";
			label = "wan";
			mtd-mac-address = <&config 0x080d>;
		};
		port@1 {
			status = "okay";
			label = "lan4";
		};
		port@2 {
			status = "okay";
			label = "lan3";
		};
		port@3 {
			status = "okay";
			label = "lan2";
		};
		port@4 {
			status = "okay";
			label = "lan1";
		};
	};
};

&state_default {
	gpio {
		groups = "rgmii2", "uart3";
		function = "gpio";
	};
};
