#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "qca956x.dtsi"

/ {
	compatible = "tplink,tl-wdr5600-v2", "qca,qca9560";
	model = "TP-Link TL-WDR5600 v2";

	aliases {
		led-boot = &system;
		led-failsafe = &system;
		led-running = &system;
		led-upgrade = &system;
		label-mac-device = &eth0;
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "Reset button";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 1 GPIO_ACTIVE_LOW>;
		};
	};

	leds {
		compatible = "gpio-leds";

		system: system {
			label = "white:system";
			gpios = <&gpio 21 GPIO_ACTIVE_LOW>;
			default-state = "on";
		};

		wan {
			label = "green:wan";
			gpios = <&gpio 19 GPIO_ACTIVE_LOW>;
		};

		lan1 {
			label = "green:lan1";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};

		lan2 {
			label = "green:lan2";
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
		};

		lan3 {
			label = "green:lan3";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
		};

		lan4 {
			label = "green:lan4";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};
	};
};

&pcie {
	status = "okay";
};

&spi {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot: factory-boot@0 {
				label = "factory_boot";
				reg = <0x000000 0x020000>;
				read-only;
			};

			config@20000 {
				label = "config";
				reg = <0x020000 0x010000>;
				read-only;
			};

			normal-boot@30000 {
				label = "normal_boot";
				reg = <0x030000 0x010000>;
				read-only;
			};

			firmware@40000 {
				label = "firmware";
				compatible = "denx,uimage";
				reg = <0x040000 0x7c0000>;
			};
		};
	};
};

&eth0 {
	status = "okay";
	phy-handle = <&swphy4>;
	mtd-mac-address = <&uboot 0x1d80d>;
};

&eth1 {
	status = "okay";
	mtd-mac-address = <&uboot 0x1d80d>;
	mtd-mac-address-increment = <1>;
};

&wmac {
	status = "okay";
	mtd-cal-data = <&uboot 0x1e000>;
	mtd-mac-address = <&uboot 0x1d80d>;
	mtd-mac-address-increment = <2>;
};
